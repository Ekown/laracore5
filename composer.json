{
    "name": "ekown/laracore5",
    "description": "Contains the core processes for Laravel 5.5 applications created by Eron Tancioco.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eron Tancioco",
            "email": "tancioco.eron@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php" : ">=7.0",
        "illuminate/support": ">=5.5",
        "monolog/monolog": "^1.21"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Ekown/laracore5.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ekown\\Laracore5\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "~3.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ekown\\Laracore5\\LaracoreServiceProvider"
            ],
            "aliases": {
                "LoginFormLogger": "Ekown\\Laracore5\\Facades\\Login\\Form",
                "LoginProcessingLogger": "Ekown\\Laracore5\\Facades\\Login\\Processing"
            }
        }
    }
}
